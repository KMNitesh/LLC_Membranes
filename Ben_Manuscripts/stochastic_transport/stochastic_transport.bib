	pages = {512--516},
	file = {Full Text PDF:/home/ben/.zotero/zotero/ahs8amtr.default/zotero/storage/RF4LVF9G/Mondloch et al. - 2015 - Destruction of chemical warfare agents using metal.pdf:application/pdf;Snapshot:/home/ben/.zotero/zotero/ahs8amtr.default/zotero/storage/ZHUE9L78/nmat4238.html:text/html}
