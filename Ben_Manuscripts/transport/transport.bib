@article{dischinger_effect_2017,
	title = {Effect of {Post}-{Polymerization} {Anion}-{Exchange} on the {Rejection} of {Uncharged} {Aqueous} {Solutes} in {Nanoporous}, {Ionic}, {Lyotropic} {Liquid} {Crystal} {Polymer} {Membranes}},
