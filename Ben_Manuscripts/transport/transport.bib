	author = {Barbosa, Marta O. and Moreira, Nuno F. F. and Ribeiro, Ana R. and Pereira, Manuel F. R. and Silva, Adrián M. T.},
	month = may,
	year = {2016},
	keywords = {Advanced oxidation processes, Contaminants of emerging concern, Conventional treatment, Decision 2015/495/EU, Fenton, ozonation and photocatalysis, Membrane technologies},
	pages = {257--279},
	file = {ScienceDirect Full Text PDF:/home/bcoscia/.zotero/zotero/tl6cqsyg.default/zotero/storage/F95SHJPN/Barbosa et al. - 2016 - Occurrence and removal of organic micropollutants.pdf:application/pdf;ScienceDirect Snapshot:/home/bcoscia/.zotero/zotero/tl6cqsyg.default/zotero/storage/MG2F98E5/S0043135416301063.html:text/html}
}

