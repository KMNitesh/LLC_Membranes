isbn = {0002-7863},
journal = {J. Am. Chem. Soc.},
number = {13},
pages = {5086--5094},
pmid = {11457339},
title = {{The prediction, morphology, and mechanical properties of the polymorphs of paracetamol}},
volume = {123},
year = {2001}
}
