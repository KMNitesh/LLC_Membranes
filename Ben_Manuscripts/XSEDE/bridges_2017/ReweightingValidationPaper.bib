@article{Day2011,
annote = {Crystal structure prediction assists in the design of materials with novel properties as well as the avoidance of undesirable change of form in the later stages of industrial development.

The structure of the crytal is the necessary starting point for understanding and predicting its physico-chemical properties.

In the literature up to 2004, most of the research focused on methods and example systems.

An overwhelming majority of crystals fall in to the packing factor of 0.65-0.75

Crystal structure prediction relies on two areas: generating all possible packings, and accurately calculating the energy of those structures.

Changes between molecules are often much larger than changes within molecules (bonds, angles etc).

In the early days, hypothetical alternatives to known crystal structures were derived from the packing of similar molecules.

One can search for the energy minima by making a grid of M points along each N degree of freedom, for M{\^{}}N total calculations. The unit cell has 6 DOF and each molecule has 6 DOF. One could also generate a random set of configurations varying these degrees of freedom, though reliably covering all of the relevant phase space can become an issue.

To accelerate this search, one can generate structures based on the known symmetry space groups. This, of course, has the potential to miss certain structures outside these space groups.

Configurations can also be generated using Monte Carlo searches combined with simulated annealing. 

Metadynamics is typically used for MD searches to predict new crystal structures (since unbiased MD becomes kinetically trapped). The free energy surface tends to be smoother than the lattice energy surface. In the case of benzene, all known polymorphs are found as free energy minima, and unobserved energy minima go away.

Most CSP methods focus on small rigid molecules. The variations to do non-rigid molecules can lead to differences of {\~{}}4 kJ/mol.

In many molecules, there are 'soft' degrees of freedom where a 20 degree rotation is permitted. The rigid molecule approach will not work here. For example, the molecule biphenyl has a 45 degree twist in the gas phase, but prefers to be planar as a crystal.

80{\%} of homomolecular organic crystal structures have either one or a fraction of a molecule in the asymmetric unit in one of the six space groups.

One way to search for polymorphs (in comercial programs) is a simulated annealing algorithm which invokes monte carlo moves on the crystal. It is recommended that this be done multiple times to ensure convergence.},
author = {Day, Graeme M.},
doi = {10.1080/0889311X.2010.517526},
file = {:C$\backslash$:/Users/Eric/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Day - 2011 - Current approaches to predicting molecular organic crystal structures.pdf:pdf},
journal = {Crystallogr. Rev.},
month = {jan},
number = {1},
pages = {3--52},
title = {{Current approaches to predicting molecular organic crystal structures}},
volume = {17},
year = {2011}
}
@article{Day2007,
abstract = {A computational exploration of the low energy crystal structures of the pharmaceutical molecule phenobarbital is presented as a test of an approach for the crystal structure prediction of flexible molecules. Traditional transferable force field methods of modelling flexible molecules are unreliable for the level of accuracy required in crystal structure prediction and we outline a strategy for improving the evaluation of relative energies of large sets of crystal structures. The approach involves treating the molecule as a set of linked rigid units, whose conformational energy is expressed as a function of the relative orientations of the rigid groups. The conformational energy is calculated by electronic structure methods and the intermolecular interactions using an atomic multipole description of electrostatics. A key consideration in our approach is the scalability to more typical pharmaceutical molecules of higher molecular weight with many more atoms and degrees of flexibility. Based on our calculations, crystal structures are proposed for the as-yet uncharacterised forms IV and V, as well as further polymorphs of phenobarbital.},
author = {Day, G M and {S Motherwell}, W D and Jones, W},
doi = {10.1039/b612190j},
file = {:C$\backslash$:/Users/Eric/Documents/Shirts Group/Papers/Computational Polymorphism/A Strategy for prediction the crystal structures of flexible molecules- the polymorphism of phenobarbital Day PCCP 2007.pdf:pdf},
isbn = {1463-9076},
journal = {Phys. Chem. Chem. Phys.},
number = {14},
pages = {1693--1704},
