@article{Cooper2008,
abstract = {Terahertz time-domain-spectroscopy (THz-TDS) has emerged as a versatile spectroscopic technique, and an alternative to powder X-ray diffraction in the characterization of molecular crystals. We tested the ability of terahertz spectroscopy to distinguish between chiral and racemic hydrogen-bonded cocrystals that are similar in molecular and supramolecular structure. Terahertz spectroscopy readily distinguished between the isostructural cocrystals of theophylline with chiral and racemic forms of malic acid which are almost identical in molecular structure and supramolecular architecture. Similarly, the cocrystals of theophylline with chiral and racemic forms of tartaric acid, which are similar at the molecular level but dissimilar in crystal packing, were distinguished unequivocally. The investigation of the same cocrystals using X-ray powder diffraction and Raman spectroscopy suggested that THz-TDS is comparable in sensitivity to diffraction methods and more sensitive than Raman spectroscopy to changes in cocrystal architecture. The differences in spectra acquired by THz-TDS could be further enhanced by cooling the samples to 109 K.},
annote = {Demonstrates that polarization is important in crystal structure prediction.

Here polarization is simily treated as a structureless polarizing continuum, however even this simple model shows promising results as the known crystal structure becomes more favorable, and the two known polymorphs are ranked as the lowest in energy.

This study argues that the intermolecular and intramolecular energy descriptions need to be 'balanced'. If intramolecular DOF are treated with QM, the lowest energy configurations all have low intramolecular energies and poor intermolecular energies, however the real crystal structure had low intermolecular DOF. Including polarizability in the intermolecular energies is an attempt to correct this imbalance. 

Even with flexible molecules, the true crystal structures are poorly represented in the DMA potential, and the true crystal structure only becomes strongly favored when polarizability is added.},
author = {Cooper, Timothy G. and Hejczyk, Katarzyna E. and Jones, William and Day, Graeme M.},
doi = {10.1021/ct800195g},
file = {:C$\backslash$:/Users/Eric/Documents/Shirts Group/Papers/Polarizability/Molecular Polarization Effects on the Relative Energies of the Real and Putative Crystal Structures of Valine Day JCTC 2008.pdf:pdf},
isbn = {1549-9618},
journal = {J. Chem. Theory Comput.},
number = {10},
pages = {1795--1805},
title = {{Molecular polarization effects on the relative energies of the real and putative crystal structures of valine}},
volume = {4},
year = {2008}
}
@article{Eike2005a,
abstract = {A rigorous and generally applicable method for computing solid-liquid coexistence is presented. The method overcomes some of the technical difficulties associated with other solid-liquid simulation procedures and can be implemented within either a molecular dynamics or Monte Carlo framework. The method consists of three steps: First, relative Gibbs free energy curves are created for the solid and liquid phases using histogram reweighting. Next, the free energy difference between the solid and liquid phases is evaluated at a single state point by integrating along a pseudosupercritical transformation path that connects the two phases. Using this result, the solid and liquid free energy curves are referenced to a common point, allowing a single coexistence point to be determined. Finally, Gibbs-Duhem integration is used to determine the full coexistence curve. To evaluate its utility, this method is applied to the Lennard-Jones and NaCl systems. Results for solid-liquid coexistence agree with previous calculations for these systems. In addition, it is shown that the NaCl model does not correctly describe solid-liquid coexistence at high pressures. An analysis of the accuracy of the method indicates that the results are most sensitive to the transformation free energy calculation.},
annote = {Gibbs ensemble and grand canonical MC have been used previously to model vapor liquid coexistence.


Direct simulations suffer from a number of problems. Interfacial type simulations suffer from system size effects, and direct phase change simulations through raising/lowering the tempreature suffer from hysteresis. This effect can be as large as 1.2x Also, the temperature rate is on the order of 10{\^{}}12 K/s


Solid free energy difference calculations are usually done either with interphase methods or with reference state methods. Typical reference states are an einstein crystal for solid phase and an ideal gas for liquid phase.


Reference state methods suffer from having to calculate the difference between reference states, which can be a huge free energy difference, while direct methods can have a much smaller difference.},
author = {Eike, David M and Brennecke, Joan F and Maginn, Edward J},
doi = {10.1063/1.1823371},
file = {:C$\backslash$:/Users/Eric/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Eike, Brennecke, Maginn - 2005 - Toward a robust and general molecular simulation method for computing solid-liquid coexistence.pdf:pdf},
journal = {J. Chem. Phys.},
keywords = {Computer Simulation,Models, Chemical,Models, Molecular,Molecular Conformation,Phase Transition,Sodium Chloride,Sodium Chloride: chemistry,Solutions,Solutions: chemistry},
month = {jan},
number = {1},
pages = {14115},
pmid = {15638650},
title = {{Toward a robust and general molecular simulation method for computing solid-liquid coexistence.}},
volume = {122},
year = {2005}
}
