@article{Beierlein2011a,
abstract = {We present a molecular simulation protocol to compute free energies of binding, which combines a QM/MM correction term with rigorous classical free energy techniques, thereby accounting for electronic polarization effects. Relative free energies of binding are first computed using classical force fields, Monte Carlo sampling, and replica exchange thermodynamic integration. Snapshots of the configurations at the end points of the perturbation are then subjected to DFT-QM/MM single-point calculations using the B3LYP functional and a range of basis sets. The resulting quantum mechanical energies are then processed using the Zwanzig equation to give free energies incorporating electronic polarization. Our approach is conceptually simple and does not require tightly coupled QM and MM software. The method has been validated by calculating the relative free energies of hydration of methane and water and the relative free energy of binding of two inhibitors of cyclooxygenase-2. Closed thermodynamic cycles are obtained across different pathways, demonstrating the correctness of the technique, although significantly more sampling is required for the protein-ligand system. Our method offers a simple and effective way to incorporate quantum mechanical effects into computed free energies of binding.},
annote = {Scoring Functions attempt to relate experimentlally determined binding affinities to calculated properties of the protein-ligand complex.


Rigorous free energy calculations in principle allow an exact computation of the binding free energy.


Free energy calculations will be off if polarization exists in the system but is not considered in the hamiltonian. In some cases, polarization can be considered by overstimating the solute gas-pase charges. 


Reweighting can also be done by generating configurations in MM and then accepting or rejecting the configurations by calculating the QM/MM Energy along with a metropolis algorithm.


Previous studies have shown that adding QM/MM corrections have led to a decrease in accuracy. Perhaps this is due to no back-polarization?


To validate this approach, the free energy difference is calculated using multiple alternative pathways to confirm that the answer is the same. This is done because sampling in the expensive potential is not possible.

The fact that both pathways give the same answer suggests that this perturbation in one step is reasonable. 

An 'ideal' charge perturbation could be done in which the free energy difference between the point-charge and polarizable model is zero.},
author = {Beierlein, Frank R and Michel, Julien and Essex, Jonathan W},
doi = {10.1021/jp109054j},
file = {:C$\backslash$:/Users/Eric/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Beierlein, Michel, Essex - 2011 - A simple QMMM approach for capturing polarization effects in protein-ligand binding free energy calcul.pdf:pdf},
journal = {J. Phys. Chem. B},
keywords = {Binding Sites,Cyclooxygenase 2,Cyclooxygenase 2 Inhibitors,Cyclooxygenase 2 Inhibitors: chemistry,Cyclooxygenase 2 Inhibitors: metabolism,Cyclooxygenase 2: chemistry,Cyclooxygenase 2: metabolism,Ligands,Monte Carlo Method,Pyrazoles,Pyrazoles: chemistry,Pyrazoles: metabolism,Quantum Theory,Software,Structure-Activity Relationship,Sulfonamides,Sulfonamides: chemistry,Sulfonamides: metabolism,Thermodynamics},
month = {may},
number = {17},
pages = {4911--4926},
