@article{Chithambararaj2016,
annote = {Polymorphism means that the materials has multiple observable crystal structures. Polymorphs can have different properties. Here the authors are looking at molybdenum oxide. In this case, the crystal structures have different electrical conductivity. The h-MoO3 phase has 550x higher conductivity than a-MoO3. 

The authors argue that the increased electrical conductivity is due to interstitial ammonia and water.

The interstitial ammonia and water causes a different charge transfer mechanism to be adopted in the h-MoO3 phase as opposed to the a-MoO3 phase. In h-MoO3 there is a bypass channel for charge transfer due to facile diffusion of ions through the matrix. In a-MoO3 only a polarization can occur. 

The authors also argue that the water and ammonium ions lead to a stabilization of the h-MoO3 phase. The evidence to support this is that heating the crystal leads to desorption of the interstitial H2O and NH4 which then leads to rapid depomposition to a-MoO3.

The remainder of the article discusses the synthesis procedure, where by the hydrothermal treatment preferentially leads to the synthesis of h-MoO3. The synthesized phase can be adjusted based on the amount of precipitant, the solvent medium, the reaction time and reaction temperature.

Pros: Lots of Colors and helpful figures

Cons: A' graphs are a bit confusing and lack detailed captions.

The presence of water and ammonium ions stabilizes the metastable phase h-MoO3. These also lead to increased electrical conductivity properties.

In the h-MoO3 phase, the water and NH4+ Ions allow electrical conduction through 'diffusion', while in the a-MoO3 phase the electrical conductino is purely through polarization which is much more resistive.},
author = {Chithambararaj, A. and {Rajeswari Yogamalar}, N. and Bose, A. Chandra},
doi = {10.1021/acs.cgd.5b01571},
file = {:C$\backslash$:/Users/Eric/Documents/Shirts Group/Papers/Experimental Polymorphism/Hydrothermally synthesized h-MoO3 and a-MoO3 Nanocrystals- New Findings on Crystal-Structure-Dependent Charge Transport Bost CGD 2016.pdf:pdf},
journal = {Cryst. Growth Des.},
pages = {acs.cgd.5b01571},
title = {{Hydrothermally Synthesized h-MoO <sub>3</sub> and $\alpha$-MoO <sub>3</sub> Nanocrystals: New Findings on Crystal-Structure-Dependent Charge Transport}},
year = {2016}
}
