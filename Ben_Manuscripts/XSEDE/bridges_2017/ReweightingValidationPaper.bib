@article{Chen2015a,
annote = {The relevant coarse-graining of the energy landscape is the free energy surface as a function of the collective variables. Even though this leads to a reduction of dimensions, this can still be a high dimensional surface. Furthermore, it can be difficult to locate these minima and saddlepoints. This allows for efficient free energy difference estimates.

The SMART algorithm developed here can identify both saddlepoints and minima on the free energy landscape. The minima can then be used for free energy difference estimates, and the minima can be used as initial guesses of transitions to determine whether minima on the energy landscape interconvert.

The algorithm finds the minima and saddlepoints by alternating between minima search and saddlepoint search algorithms. This can be done with MC or MD, but MD is used here.

Traditional optimizations have decreasing timesteps, but here a fixed timestep with a normalized force is used.

Walking along in this trajectory leads to a cluster of configurations around minima and saddlepoints, which are called 'landmarks'.},
author = {Chen, Ming and Yu, Tang-Qing and Tuckerman, Mark E.},
doi = {10.1073/pnas.1418241112},
file = {:C$\backslash$:/Users/Eric/Documents/Shirts Group/Papers/Solid Free Energy Difference Methods/Locating landmarks on high-dimensional free energy surfaces Tuckerman PNAS 2015.pdf:pdf},
journal = {Proc. Natl. Acad. Sci.},
number = {11},
pages = {3235--3240},
title = {{Locating landmarks on high-dimensional free energy surfaces}},
volume = {112},
year = {2015}
}
