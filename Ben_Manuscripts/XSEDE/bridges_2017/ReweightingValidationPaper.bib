keywords = {Antimetabolites,Antineoplastic,Antineoplastic: chemistry,Computer Simulation,Crystallization,Dimerization,Fluorouracil,Fluorouracil: chemistry,Hydrogen Bonding,Kinetics,Methane,Methane: analogs {\&} derivatives,Methane: chemistry,Models,Molecular,Molecular Conformation,Molecular Structure,Nitroparaffins,Nitroparaffins: chemistry,Software,Solvents,Solvents: chemistry,Water,Water: chemistry},
month = {feb},
number = {7},
pages = {3323--3329},
