@article{Luzhkov1992,
abstract = {Our previously developed approaches for integrating quantum mechanical$\backslash$nmolecular orbital methods with microscopic solvent models are refined$\backslash$nand examined. These approaches consider the nonlinear solute-solvent$\backslash$ncoupling in a self-consistent way by incorporating the potential$\backslash$nfrom the solvent dipoles in the solute Hamiltonian, while considering$\backslash$nthe polarization of the solvent by the potential from the solute$\backslash$ncharges. The solvent models used include the simplified Langevin$\backslash$nDipoles (LD) model and the much more expensive surface constrained$\backslash$nAll Atom Solvent (SCAAS) model, which is combined with a free energy$\backslash$npertubation (FEP) approach. Both methods are effectively integrated$\backslash$nwith the quantum mechanical AMPAC package and can be easily combined$\backslash$nwith other quantum mechanical programs. The advantages of the present$\backslash$napproaches and their earlier versions over macroscopic reaction field$\backslash$nmodels and supermolecular approaches are considered. A LD/MNDO study$\backslash$nof solvated organic ions demonstrates that this model can yield reliable$\backslash$nsolvation energies, provided the quantum mechanical charges are scaled$\backslash$nto have similar magnitudes to those obtained by high level ab initio$\backslash$nmethods. The incorporation of a field-dependent hydrophobic term$\backslash$nin the LD free energy makes the present approach capable of evaluating$\backslash$nthe free energy of transfer of polar molecules from non polar solvents$\backslash$nto aqueous solutions. The reliability of the LD approach is examined$\backslash$nnot only by evaluating a rather standard set of solvation energies$\backslash$nof organic ions and polar molecules, but also by considering the$\backslash$nstringent test case of sterically hindered hydrophobic ions. In this$\backslash$ncase, we compare the LD/MNDO solvation energies to the more rigorous$\backslash$nFEP/SCAAS/MNDO solvation energies. Both methods are found to give$\backslash$nsimilar results even in this challenging test case. The FEP/SCAAS/AMPAC$\backslash$nmethod is incorporated into the current version of the program ENZYMIX.$\backslash$nThis option allows one to study chemical reactions in enzymes and$\backslash$nin solutions using the MNDO and AM1 approximations. A special procedure$\backslash$nthat uses the EVB method as a reference potential for SCF MO calculations$\backslash$nshould help in improving the reliability of such studies.},
author = {Luzhkov, V and Luzhkov, V and Warshel, A and Warshel, A},
doi = {10.1002/jcc.540130212},
file = {:C$\backslash$:/Users/Eric/Documents/Shirts Group/Papers/Reweighting/Microscopic Model for QM Calculations of Chemical Processes in Solutions LDAMPAC and SCAASAMPAC Calculations of Solvation Energies Warshel JCC 1992.pdf:pdf},
isbn = {0192-8651},
journal = {J. Comput. Chem.},
number = {2},
pages = {199--213},
title = {{Microscopic models for quantum mechanical calculations of chemical processes in solutions: LD/AMPAC and SCAAS/AMPAC calculations of solvation energies}},
volume = {13},
year = {1992}
}
@article{Hudson2015,
abstract = {BACKGROUND: Accurately modeling condensed phase processes is one of computation's most difficult challenges. Include the possibility that conformational dynamics may be coupled to chemical reactions, where multiscale (i.e., QM/MM) methods are needed, and this task becomes even more daunting.$\backslash$n$\backslash$nMETHODS: Free energy simulations (i.e., molecular dynamics), multiscale modeling, and reweighting schemes.$\backslash$n$\backslash$nRESULTS: Herein, we present two new approaches for mitigating the aforementioned challenges. The first is a new chain-of-replica method (off-path simulations, OPS) for computing potentials of mean force (PMFs) along an easily defined reaction coordinate. This development is coupled with a new distributed, highly-parallel replica framework (REPDstr) within the CHARMM package. Validation of these new schemes is carried out on two processes that undergo conformational changes. First is the simple torsional rotation of butane, while a much more challenging glycosidic rotation (in vacuo and solvated) is the second. Additionally, a new approach that greatly improves (i.e., possibly an order of magnitude) the efficiency of computing QM/MM PMFs is introduced and compared to standard schemes. Our efforts are grounded in the recently developed method for efficiently computing QM-based free energies (i.e., QM-Non-Boltzmann Bennett, QM-NBB). Again, we validate this new technique by computing the QM/MM PMF of butane's torsional rotation.$\backslash$n$\backslash$nCONCLUSIONS: The OPS-REPDstr method is a promising new approach that overcomes many limitations of standard pathway simulations in CHARMM. The combination of QM-NBB with pathway techniques is very promising as it offers significant advantages over current procedures.$\backslash$n$\backslash$nGENERAL SIGNIFICANCE: Efficiently computing potentials of mean force is a major, unresolved, area of interest. This article is part of a Special Issue entitled Recent developments of molecular dynamics.},
author = {Hudson, Phillip S and White, Justin K and Kearns, Fiona L and Hodoscek, Milan and Boresch, Stefan and {Lee Woodcock}, H},
doi = {10.1016/j.bbagen.2014.09.016},
file = {:C$\backslash$:/Users/Eric/Documents/Shirts Group/Papers/Reweighting/Efficiently computing pathway free energies- New approaches based on chain-of-replica and Non-Boltzmann Bennett reweighting schemes Woodcock BBA 2015.pdf:pdf},
journal = {Biochim. Biophys. Acta},
keywords = {BAR,Bennett's acceptance ratio,Free energy,Potential of mean force,QM-NBB,QM-Non-Boltzmann Bennett,QM/MM,Reaction path,Reweighting},
number = {5},
pages = {944--53},
pmid = {25239198},
publisher = {Elsevier B.V.},
