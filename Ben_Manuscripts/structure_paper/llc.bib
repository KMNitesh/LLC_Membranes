@article{han_transfer_2016,
	title = {Transfer of neutral organic solutes during desalination by electrodialysis: {Influence} of the salt composition},
	volume = {511},
	issn = {0376-7388},
	shorttitle = {Transfer of neutral organic solutes during desalination by electrodialysis},
	url = {http://www.sciencedirect.com/science/article/pii/S0376738816301892},
	doi = {10.1016/j.memsci.2016.03.053},
	abstract = {Electrodialysis is a promising process to treat saline water containing organic solute. The desalination performances are fixed by the transfer of salts and organic solutes. On the contrary to the transfer of salts, few results were reported regarding that of organic compounds. This is the objective of this work to investigate the transfer of neutral organic solutes (acetic acid, phenol, glucose) through ion-exchange membranes, focusing on the influence of the salt (NaCl, MgCl2, Na2SO4). Results show that the water transfer due to electroosmosis depends on the salt composition following the ion hydration. Two contributions, diffusion and convection, are pointed out for the transfer of organic solutes. Both are important and mainly fixed by steric effect. Concerning the influence of the ion hydration, reverse trends are found for both contributions. Decreasing diffusion flux is observed for increasing membrane counter-ion hydration showing the influence of the membrane structural properties. Increasing convection flux is observed for increasing ion hydration showing the influence of the solute hydration.},
	urldate = {2016-04-15},
	journal = {Journal of Membrane Science},
	author = {Han, Le and Galier, Sylvain and Roux-de Balmann, Hélène},
	month = aug,
	year = {2016},
	keywords = {Desalination, Electrodialysis, Hydration, Mass transfer, Organic solute},
	pages = {207--218},
	file = {ScienceDirect Full Text PDF:/home/bcoscia/.zotero/zotero/tl6cqsyg.default/zotero/storage/PA43SJX9/Han et al. - 2016 - Transfer of neutral organic solutes during desalin.pdf:application/pdf;ScienceDirect Snapshot:/home/bcoscia/.zotero/zotero/tl6cqsyg.default/zotero/storage/PEIU8ZQP/S0376738816301892.html:text/html}
}

