@article{rustichelli_solid-state_2000,
	title = {Solid-state study of polymorphic drugs: carbamazepine},
	volume = {23},
	issn = {0731-7085},
	shorttitle = {Solid-state study of polymorphic drugs},
	url = {http://www.sciencedirect.com/science/article/pii/S0731708500002624},
	doi = {10.1016/S0731-7085(00)00262-4},
	abstract = {Polymorphs of a compound have solid crystalline phases with different internal crystal lattices; in pharmaceuticals, differences due to polymorphism and pseudopolymorphism can affect bioavailability and effective clinical use. The aim of this work was to obtain the different polymorphic modifications of the anticonvulsant drug, carbamazepine, and to characterise them by means of typical structure-sensitive analytical techniques, such as FT-IR spectroscopy, XRPD and DSC. Further investigations were also performed by Hot Stage FT-IR thermomicroscopy, which permitted the visible and spectroscopic characterisation of the polymorphic forms during heating. Our results confirm the existence of three different polymorphic forms for anhydrous carbamazepine: Form III, the commercial one, Form I, obtained by heating Form III and Form II, crystallised from ethanolic solution. Substantial differences were detected among the polymorphs with regard to solid-state properties. Moreover, Hot Stage FT-IR thermomicroscopy proved its analytical potential to characterise the drug's polymorphism.},
	number = {1},
	urldate = {2016-10-13},
	journal = {Journal of Pharmaceutical and Biomedical Analysis},
