	author = {Smolders, C. A. and Reuvers, A. J. and Boom, R. M. and Wienk, I. M.},
	month = oct,
	year = {1992},
	keywords = {macrovoids, membrane preparation and structure, phase-inversion membranes, theory, thermodynamics},
	pages = {259--275},
	file = {ScienceDirect Full Text PDF:/home/bcoscia/.zotero/zotero/tl6cqsyg.default/zotero/storage/VSXUMC5U/Smolders et al. - 1992 - Microstructures in phase-inversion membranes. Part.pdf:application/pdf;ScienceDirect Snapshot:/home/bcoscia/.zotero/zotero/tl6cqsyg.default/zotero/storage/AQ9EDV99/0376738892801346.html:text/html}
}

