	pages = {1393--1407},
	file = {ScienceDirect Full Text PDF:/home/bcoscia/.zotero/zotero/tl6cqsyg.default/zotero/storage/UV4AH4RE/Bowen and Welfoot - 2002 - Modelling of membrane nanofiltration—pore size dis.pdf:application/pdf;ScienceDirect Snapshot:/home/bcoscia/.zotero/zotero/tl6cqsyg.default/zotero/storage/7J9QVEJP/S0009250901004122.html:text/html}
}

