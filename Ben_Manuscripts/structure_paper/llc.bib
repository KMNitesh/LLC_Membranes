	keywords = {Alzheimer’s disease, amyloid-related proteins, aromatic residue},
	pages = {77--83},
	file = {Full Text PDF:/home/bcoscia/.zotero/zotero/tl6cqsyg.default/zotero/storage/RGNFF6DX/Gazit - 2002 - A possible role for π-stacking in the self-assembl.pdf:application/pdf;Snapshot:/home/bcoscia/.zotero/zotero/tl6cqsyg.default/zotero/storage/J63VHMBJ/77.html:text/html}
}

@article{sinnokrot_estimates_2002-1,
	title = {Estimates of the {Ab} {Initio} {Limit} for π−π {Interactions}:  {The} {Benzene} {Dimer}},
