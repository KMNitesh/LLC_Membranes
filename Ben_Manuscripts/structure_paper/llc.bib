	pages = {4667--4673},
	file = {ACS Full Text PDF w/ Links:/home/bcoscia/.zotero/zotero/tl6cqsyg.default/zotero/storage/2XR5KGJH/Zhu et al. - 2006 - Methacrylated Self-Organizing 2,3,4-Tris(alkoxy)be.pdf:application/pdf;ACS Full Text Snapshot:/home/bcoscia/.zotero/zotero/tl6cqsyg.default/zotero/storage/MP3IT83N/cm060305y.html:text/html}
