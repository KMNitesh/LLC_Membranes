	month = aug,
	year = {2000},
	keywords = {Polymorphism, Carbamazepine, Solid state characterisation, Hot Stage FT-IR thermomicroscopy, X-Ray powder diffraction, Differential scanning calorimetry},
	pages = {41--54},
	file = {ScienceDirect Full Text PDF:/home/bcoscia/.zotero/zotero/tl6cqsyg.default/zotero/storage/2R8QJ3GH/Rustichelli et al. - 2000 - Solid-state study of polymorphic drugs carbamazep.pdf:application/pdf;ScienceDirect Snapshot:/home/bcoscia/.zotero/zotero/tl6cqsyg.default/zotero/storage/Z58FIVZ7/S0731708500002624.html:text/html}
