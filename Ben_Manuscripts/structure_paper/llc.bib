@article{zuo_transport_2010,
	title = {Transport properties of single-file water molecules inside a carbon nanotube biomimicking water channel.},
	volume = {4},
	issn = {1936-086X},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/20000381},
	doi = {10.1021/nn901334w},
	abstract = {The single-file water transport through a biomimic water channel consisting of a (6,6) carbon nanotube (CNT) with different types of external point charges is studied using molecular dynamics simulations. It is demonstrated that, as in the aquaporins, asymmetrically positioned charges cannot generate robust unidirectional water flow in the CNT. Thermal fluctuation in bulk water competes with charge affinity to steer the water transport, resulting in nonmonotonic flow with intermittent reversal of transport direction. The energetic analysis suggests that the water-water interaction, determined by dipole orientation configuration, influences the transport rate significantly. These findings can provide correct biomimic understanding of water transport properties and will benefit the design of efficient functional nanofluidic devices.},
	number = {1},
	journal = {ACS nano},
	author = {Zuo, Guangchao and Shen, Rong and Ma, Shaojie and Guo, Wanlin},
	year = {2010},
	pmid = {20000381},
	keywords = {Aquaporins, Aquaporins: chemistry, Biomimetics, Carbon, Carbon: chemistry, Kinetics, Microfluidic Analytical Techniques, Molecular Dynamics Simulation, Nanotubes, Porins, Porins: chemistry, Static Electricity, Thermodynamics, Water, Water: analysis, Water: metabolism},
	pages = {205--10},
	file = {Attachment:/home/bcoscia/.zotero/zotero/tl6cqsyg.default/zotero/storage/HZXM3VK4/Relate CNT to biological channels.pdf:application/pdf}
}

@article{joseph_why_2008,
	title = {Why are carbon nanotubes fast transporters of water?},
	volume = {8},
	issn = {1530-6984},
	url = {http://dx.doi.org/10.1021/nl072385q},
	doi = {10.1021/nl072385q},
	abstract = {Extraordinarily fast transport of water in carbon nanotubes (CNTs) in recent experiments has been generally attributed to the smoothness of the CNT surface. Using molecular dynamics simulations we investigate water flow in (16,16) CNTs and show that the enhanced flow rates over Hagen-Poiseuille flow arise from a velocity "jump" in a depletion region at the water nanotube interface and that the water orientations and hydrogen bonding at the interface significantly affect the flow rates. For nanotube with the same smooth wall structure but with more hydrophilic Lennard-Jones (LJ) parameters of silicon, the enhancement is greatly reduced because it does not have "free" OH bonds pointing to the wall as in CNTs that would reduce the number of hydrogen bonds in the depletion layer. Roughness in the tube walls causes strong hydrogen-bonding network and no significant flow enhancement is attained in rough tubes.},
	number = {2},
	journal = {Nano letters},
	author = {Joseph, Sony and Aluru, N R},
	year = {2008},
	pmid = {18189436},
	keywords = {Carbon, Carbon: chemistry, Chemical, Computer Simulation, Diffusion, Microfluidics, Microfluidics: methods, Models, Molecular, Nanotubes, Porosity, Pressure, Water, Water: chemistry},
	pages = {452--8},
	file = {Attachment:/home/bcoscia/.zotero/zotero/tl6cqsyg.default/zotero/storage/DN839MSZ/Why are CNTs such fast transporters of water.pdf:application/pdf}
}

