	keywords = {Atomic force microscopy, Desalination, Fouling, Modeling, Nanofiltration, Pretreatment},
	pages = {281--308},
	file = {ScienceDirect Snapshot:/home/bcoscia/.zotero/zotero/tl6cqsyg.default/zotero/storage/WBVNBRBD/S0011916404800328.html:text/html}
}

