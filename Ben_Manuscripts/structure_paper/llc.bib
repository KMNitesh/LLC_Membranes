	keywords = {Anti-Infective Agents, Anti-Infective Agents: chemical synthesis, Anti-Infective Agents: chemistry, Biological, Foldamers, Humans, Methane, Methane: chemistry, Models, Peptidomimetics, Peptoids, Peptoids: chemistry, Protein Folding},
