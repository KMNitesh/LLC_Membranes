@article{chodera_simple_2016-1,
	title = {A {Simple} {Method} for {Automated} {Equilibration} {Detection} in {Molecular} {Simulations}},
	volume = {12},
	issn = {1549-9618},
	url = {http://dx.doi.org/10.1021/acs.jctc.5b00784},
	doi = {10.1021/acs.jctc.5b00784},
	abstract = {Molecular simulations intended to compute equilibrium properties are often initiated from configurations that are highly atypical of equilibrium samples, a practice which can generate a distinct initial transient in mechanical observables computed from the simulation trajectory. Traditional practice in simulation data analysis recommends this initial portion be discarded to equilibration, but no simple, general, and automated procedure for this process exists. Here, we suggest a conceptually simple automated procedure that does not make strict assumptions about the distribution of the observable of interest in which the equilibration time is chosen to maximize the number of effectively uncorrelated samples in the production timespan used to compute equilibrium averages. We present a simple Python reference implementation of this procedure and demonstrate its utility on typical molecular simulation data.},
	number = {4},
	urldate = {2017-12-19},
	journal = {J. Chem. Theory Comput.},
	author = {Chodera, John D.},
	month = apr,
	year = {2016},
	pages = {1799--1805},
	file = {ACS Full Text PDF w/ Links:/home/bcoscia/.zotero/zotero/tl6cqsyg.default/zotero/storage/8T3UE57Q/Chodera - 2016 - A Simple Method for Automated Equilibration Detect.pdf:application/pdf;ACS Full Text Snapshot:/home/bcoscia/.zotero/zotero/tl6cqsyg.default/zotero/storage/GXSY4MCR/acs.jctc.html:text/html}
}

