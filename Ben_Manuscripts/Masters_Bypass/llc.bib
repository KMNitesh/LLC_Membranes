	author = {Chemburkar, Sanjay R. and Bauer, John and Deming, Kris and Spiwek, Harry and Patel, Ketan and Morris, John and Henry, Rodger and Spanton, Stephen and Dziki, Walter and Porter, William and Quick, John and Bauer, Phil and Donaubauer, John and Narayanan, B. A. and Soldani, Mauro and Riley, Dave and McFarland, Kathyrn},
	month = sep,
