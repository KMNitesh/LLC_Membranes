@article{blondel_ensemble_2004,
	title = {Ensemble variance in free energy calculations by thermodynamic integration: theory, optimal alchemical path, and practical solutions.},
	volume = {25},
	abstract = {Thermodynamic integration is a widely used method to calculate and analyze the effect of a chemical modification on the free energy of a chemical or biochemical process, for example, the impact of an amino acid substitution on protein association. Numerical fluctuations can introduce large uncertainties, limiting the domain of application of the method. The parametric energy function describing the chemical modification in the thermodynamic integration, the “alchemical path”, determines the amplitudes of the fluctuations. In the present work, I propose a measure of the fluctuations in the thermodynamic integration and an approach to search for a parametric energy path minimizing that measure. The optimal path derived with this approach is very close to the theoretical minimum of the measure, but produces nonergodic sampling. Nevertheless, this path is used to guide the design of a practical and efficient path producing correct sampling. The convergence with this practical path is evaluated on test cases, and compares favorably with that of other methods such as power or polynomial path, soft-core van der Waals, and some other approaches presented in the literature. (15 refs.)},
	number = {7},
	journal = {Journal Of Computational Chemistry},
	author = {Blondel, A.},
	year = {2004},
	keywords = {Alchemical path- Amino acid substitution impact- A, and active sites (A8715B), Chemical modification effect, Chemical process, condensed states (A8715D), configuration, conformation, dynamics, Energy states, Ensemble variance, Fluctuation amplitude, fluctuations, Free energy, Free Energy Calculation, free energy calculations, General mathematical and computational development, Interatomic and intermolecular potentials and forc, macromolecules, Model reactions in molecular biophysics (A8715P), molecular force constants, Nonergodic sampling, Optimal alchemical path, Parametric energy function, Parametric energy path, Physical chemistry of biomolecular solutions, Polynomial path, Potential energy functions, Power path, prediction variance, Protein association, Proteins, SIMULATIONS, Soft core van der Waals, STATISTICAL ERRORS, Statistical physics, thermodynamic integration, Thermodynamics, VAN DER WAALS FORCES},
	pages = {985 -- 993}
}

