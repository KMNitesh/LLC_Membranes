@article{swanekamp_coassembly_2012,
	title = {Coassembly of {Enantiomeric} {Amphipathic} {Peptides} into {Amyloid}-{Inspired} {Rippled} \textit{β}-{Sheet} {Fibrils}.},
	issn = {1520-5126},
	url = {http://dx.doi.org/10.1021/ja301642c},
	abstract = {Amphipathic peptides composed of alternating hydrophobic and hydrophilic amino acids self-assemble into amyloid-inspired, \textit{β}-sheet nanoribbon fibrils. Herein, we report a new fibril type that is formed from equimolar mixtures of enantiomeric amphipathic peptides (L- and D-(FKFE)$_{\textrm{2}}$). Spectroscopic analysis indicates that these peptides do not self-sort and assemble into enantiomeric fibrils composed of all-L and all-D peptides, but rather coassemble into fibrils that contain alternating L- and D-peptides in a "rippled \textit{β}-sheet" orientation. Isothermal titration calorimetry indicates an enthalpic advantage for rippled \textit{β}-sheet coassembly compared to self-sorted \textit{β}-sheet assembly of enantiomeric peptides.},
